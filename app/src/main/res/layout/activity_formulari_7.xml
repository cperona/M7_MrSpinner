<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".FormulariActivity_7">
    <EditText
            android:layout_width="302dp"
            android:layout_height="48dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/userNameEditText"
            android:hint="name" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="84dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="103dp" android:layout_marginEnd="98dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintHorizontal_bias="0.456"
            android:layout_marginBottom="17dp" app:layout_constraintBottom_toTopOf="@+id/surnameEditText"/>
    <EditText
            android:layout_width="302dp"
            android:layout_height="48dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/surnameEditText"
            android:hint="surname"
            app:layout_constraintStart_toStartOf="@+id/userNameEditText"
            app:layout_constraintEnd_toEndOf="@+id/userNameEditText" android:layout_marginTop="17dp"
            app:layout_constraintTop_toBottomOf="@+id/userNameEditText"
            app:layout_constraintHorizontal_bias="0.0"/>
    <EditText
            android:layout_width="302dp"
            android:layout_height="48dp"
            android:inputType="textEmailAddress"
            android:ems="10"
            android:id="@+id/emailEditText"
            android:hint="email"
            android:layout_marginTop="26dp"
            app:layout_constraintTop_toBottomOf="@+id/editTextPhone" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="61dp" android:layout_marginEnd="48dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginBottom="21dp"
            app:layout_constraintBottom_toTopOf="@+id/editTextTextPostalAddress"/>
    <EditText
            android:layout_width="302dp"
            android:layout_height="48dp"
            android:inputType="phone"
            android:ems="10"
            android:id="@+id/editTextPhone"
            android:hint="phone" app:layout_constraintStart_toStartOf="@+id/surnameEditText"
            app:layout_constraintEnd_toEndOf="@+id/surnameEditText" android:layout_marginTop="17dp"
            app:layout_constraintTop_toBottomOf="@+id/surnameEditText"/>
    <EditText
            android:layout_width="302dp"
            android:layout_height="48dp"
            android:inputType="textPostalAddress"
            android:ems="10"
            android:id="@+id/editTextTextPostalAddress"
            android:hint="address" app:layout_constraintStart_toStartOf="@+id/emailEditText"
            app:layout_constraintEnd_toEndOf="@+id/emailEditText" android:layout_marginTop="20dp"
            app:layout_constraintTop_toBottomOf="@+id/emailEditText"/>
    <TextView
            android:text="@string/proteccioDades"
            android:layout_width="302dp"
            android:layout_height="57dp" android:id="@+id/textView5"
            app:layout_constraintStart_toStartOf="@+id/editTextTextPostalAddress"
            app:layout_constraintEnd_toEndOf="@+id/editTextTextPostalAddress"
            app:layout_constraintTop_toBottomOf="@+id/editTextTextPostalAddress" android:layout_marginTop="36dp"
            app:layout_constraintHorizontal_bias="1.0" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.065"/>
    <Spinner
            android:layout_width="211dp"
            android:layout_height="21dp" android:id="@+id/genderSpinner"
            android:entries="@array/spinnerStrings" tools:layout_editor_absoluteY="525dp"
            tools:layout_editor_absoluteX="101dp"/>

</androidx.constraintlayout.widget.ConstraintLayout>